                <TableColumn prefWidth="125.0" text="Nombre">
                	<cellValueFactory>
                		<PropertyValueFactory property="nombre" />
                	</cellValueFactory> 
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Peso">
                	<cellValueFactory>
                		<PropertyValueFactory property="peso" />
                	</cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Precio">
                	<cellValueFactory>
                		<PropertyValueFactory property="precio" />
                	</cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Existencias">
      					<cellValueFactory>
                		<PropertyValueFactory property="existencias" />
                	</cellValueFactory>                
                </TableColumn>
                <TableColumn prefWidth="75.0" text="Marca">
      					<cellValueFactory>
                		<PropertyValueFactory property="marca" />
                	</cellValueFactory>                
                </TableColumn>
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <Button fx:id="bsalir" layoutX="434.0" layoutY="257.0" mnemonicParsing="false" onAction="#salir" text="Salir" />
   </children>
</AnchorPane>
